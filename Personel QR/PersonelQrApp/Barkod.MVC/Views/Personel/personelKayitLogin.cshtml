    <script>

        //Update
        var loginBtn = function () {
            $('#k_ad').empty();
            $('#k_sifre').empty();
            var kadi = $('#k_ad').val();
            var ksifre =  $('#k_sifre').val();

            $.ajax({
                url: '/personel/personelKayitLogin',
                data: { kadi: kadi, ksifre: ksifre },
                type: 'POST',
                dataType: 'Json',
                beforeSend: function () {
                    if (kadi != null && ksifre) {

                        $("#load_animation").show();
                        $("#load_animation").hide();
                    } else {
                        $("#load_animation").hide();
                    }
                },
                success: function (data) {
                    window.location.href = "Personel/Index";
                    $("#load_animation").hide();
                }
            })

        }



              $(function () {
        var SuccessMessage = '@TempData["SuccessMessage"]'
        if (SuccessMessage != '')
                     toastr.success(SuccessMessage);

          var InfoMessage = '@TempData["InfoMessage"]'
         if (InfoMessage != '')
                     toastr.info(InfoMessage);

                         var WarnMessage = '@TempData["WarnMessage"]'
                 if (WarnMessage != '')
                     toastr.warning(WarnMessage);

              var ErrorMessage = '@TempData["ErrorMessage"]'
        if (ErrorMessage != '')
             toastr.error(ErrorMessage);
     })

    </script>


